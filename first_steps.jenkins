#!groovy
//Check ubntsrv properties
properties([disableConcurrentBuilds()])

pipeline {
	agent {
		label 'master'
		}
	options {
		buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
		timestamps()
	}
	stages {
		stage("First step") {
			steps {
				sh 'ssh root@ubntsrv2 \'hostname\''
			}
		}
		stage("Second step") {
			steps {
				sh 'ssh root@ubntsrv2 \'uptime\''
			}
		}
	}
}
